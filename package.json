{
  "name": "jtc-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js && cp build/index.html build/404.html",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "lint": "eslint . --max-warnings 0",
    "test-src": "jest --coverage",
    "test-watch": "yarn test-src --watch",
    "test": "yarn lint && yarn typecheck && yarn test-src",
    "typecheck": "flow"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "dotenv": "4.0.0",
    "downshift": "^1.30.0",
    "enzyme": "^3.3.0",
    "fs-extra": "3.0.1",
    "google-map-react": "^0.34.0",
    "history": "^4.7.2",
    "hot-formula-parser": "^2.3.1",
    "immutability-helper": "^2.6.4",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.39",
    "material-ui-custom-datepicker": "^1.0.27",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.22.0",
    "normalizr": "^3.2.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "promise": "8.0.1",
    "prop-types": "latest",
    "query-string": "^5.1.0",
    "raf": "3.4.0",
    "raven-js": "^3.23.1",
    "react": "latest",
    "react-addons-update": "^15.6.2",
    "react-block-ui": "^1.1.1",
    "react-breadcrumbs": "^2.1.4",
    "react-data-export": "^0.3.2",
    "react-datetime": "^2.12.0",
    "react-dom": "^16.3.0",
    "react-easy-chart": "^1.0.0",
    "react-google-maps": "^9.4.5",
    "react-materialui-notifications": "^0.5.1",
    "react-native-web": "^0.5.1",
    "react-number-format": "^3.2.0",
    "react-paginate": "^5.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^1.2.1",
    "react-swipeable-views": "^0.12.12",
    "react-tap-event-plugin": "^3.0.2",
    "react-times": "^3.0.0",
    "react-tooltip": "^3.4.0",
    "recharts": "^1.0.0-beta.10",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "typeface-roboto": "0.0.54",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-react-native-web": "^0.5.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "compression-webpack-plugin": "^1.1.3",
    "css-loader": "0.28.7",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.10",
    "flow-bin": "^0.56.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "postcss-loader": "2.0.8",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-dev-utils": "^4.2.1",
    "react-to-print": "^1.0.10",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  }
}
